<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.wenzhencn.cmsseed.dev.mapper.DictMapper">

    <select id="selectMaxOrderByType" resultType="java.lang.Integer">
        SELECT MAX(`order`) FROM sys_dict WHERE `type` = #{type}
    </select>

</mapper>
